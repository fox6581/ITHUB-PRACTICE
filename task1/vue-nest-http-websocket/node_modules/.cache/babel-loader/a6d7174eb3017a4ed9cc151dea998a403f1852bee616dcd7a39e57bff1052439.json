turn {\r\n      keyword: \"\",\r\n      status: {\r\n        size: 0,\r\n        currentThreads: 0,\r\n        progress: 0,\r\n      },\r\n      contentList: [],\r\n      selectedContent: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async downloadFile(url) {\r\n      try {\r\n        const response = await fetch(`http://localhost:3000/download/${encodeURIComponent(url)}`, {\r\n          method: 'GET',\r\n        });\r\n\r\n        if (response.status === 200) {\r\n          // Если запрос успешен, получаем файл\r\n          const blob = await response.blob();\r\n\r\n          // Создаем ссылку для скачивания файла\r\n          const fileURL = window.URL.createObjectURL(blob);\r\n\r\n          // Создаем ссылку для скачивания\r\n          const a = document.createElement('a');\r\n          a.href = fileURL;\r\n          a.download = 'filename.jpg'; // Замените на имя файла, которое вы хотите сохранить\r\n\r\n          // Добавляем ссылку на страницу и автоматически кликаем на нее\r\n          document.body.appendChild(a);\r\n          a.click();\r\n\r\n          // Очищаем созданные ресурсы\r\n          window.URL.revokeObjectURL(fileURL);\r\n          document.body.removeChild(a);\r\n        } else {\r\n          console.error('Ошибка скачивания файла');\r\n        }\r\n      } catch (error) {\r\n        console.error(`Произошла ошибка: ${error}`);\r\n      }\r\n    },\r\n    sendMessage() {\r\n      if (this.keyword) {\r\n        const socket = new WebSocket(\"ws://localhost:3000/keywords\");\r\n        socket.addEventListener(\"open\", () => {\r\n          socket.send(this.keyword);\r\n          this.contentList = [];\r\n        });\r\n\r\n        socket.addEventListener(\"message\", (event) => {\r\n          const data = JSON.parse(event.data);\r\n          console.log(\"Получены данные от сервера:\", data);\r\n\r\n          if (Array.isArray(data) && data.length > 0) {\r\n            this.status = {\r\n              size: data[0].size,\r\n              currentThreads: data[0].currentThreads,\r\n              progress: data[0].progress,\r\n            };\r\n            this.contentList = data;\r\n          } else {\r\n            this.status = {\r\n              size: 0,\r\n              currentThreads: 0,\r\n              progress: 0,\r\n            };\r\n            this.contentList = [];\r\n          }\r\n        });\r\n\r\n        socket.addEventListener(\"error\", () => {\r\n          console.log(\"Ошибка соединения\");\r\n        });\r\n      }\r\n    },\r\n\r\n    selectContent(urls) {\r\n      // Выберите URL, который пользователь хочет скачать\r\n      const selectedUrl = urls[0]; // В данном примере выбираем первый URL\r\n\r\n      // Отправьте выбранный URL на сервер для скачивания\r\n      const socket = new WebSocket(\"ws://localhost:3000/keywords\");\r\n      socket.addEventListener(\"open\", () => {\r\n        // Отправьте URL на сервер для скачивания\r\n        socket.send(selectedUrl);\r\n      });\r\n\r\n\r\n      socket.addEventListener(\"message\", (event) => {\r\n        const data = JSON.parse(event.data);\r\n        console.log(\"Получены данные от сервера:\", data);\r\n\r\n        // Обновите статус загрузки\r\n        this.status = {\r\n          size: data.size,\r\n          currentThreads: data.currentThreads,\r\n          progress: data.progress,\r\n        };\r\n\r\n        // Сохраните скачанный контент в selectedContent\r\n        this.selectedContent = data.content;\r\n      });\r\n\r\n      socket.addEventListener(\"error\", () => {\r\n        console.log(\"Ошибка соединения\");\r\n      });\r\n    },\r\n\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;;;;;;;;;gCAgBMA,mBAAA,CAA2B,YAAvB,oBAAkB;;;uBAf1BC,mBAAA,CAsBM,c,gBArBJD,mBAAA,CAAkE;+DAAlDE,KAAA,CAAAC,OAAO,GAAAC,MAAA;IAAEC,WAAW,EAAC;iDAArBH,KAAA,CAAAC,OAAO,E,GACvBH,mBAAA,CAAoE;IAA3DM,QAAQ,GAAGJ,KAAA,CAAAC,OAAO;IAAGI,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;KAAE,WAAS,iBAAAG,UAAA,GAC